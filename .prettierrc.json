{
  "printWidth": 119,
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "insertPragma": false,
  "requirePragma": false,
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "overrides": [
    {
      "files": [
        "*.yml",
        "*.yaml"
      ],
      "options": {
        "tabWidth": 2,
        "singleQuote": false
      }
    },
    {
      "files": [
        "*.json"
      ],
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": [
        "*.md"
      ],
      "options": {
        "printWidth": 119,
        "proseWrap": "preserve",
        "tabWidth": 2
      }
    }
  ]
}